<tr>
	<td><%= link_to(product.title,product_path(product))%></td>
	<td><%= product.description%></td>
	<td><%= product.try(:country)%></td>
	<td><%= product.price%></td>
	<td><%= product.published%></td>
	<td><%= product.category.try(:title) %></td>
	<td><%= link_to('Edit', edit_product_path(product))%></td>
	<td><%= link_to('Delete', product_path(product), method: :delete, data: { confirm: 'Are you sure want to delete this product?'})%></td>
</tr>